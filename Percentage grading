#include<stdio.h>
void main()
{
    int marks;
    printf("Enter your marks ");
    scanf("%d",&marks);
    if(marks<0 || marks>100)// when other numbers were entered
    {
        printf("Wrong Entry");
    }
    else if(marks<40)// for grade F
    {
        printf("Grade F");
    }
    else if(marks>=40 && marks<54)//for grade D
    {
        printf("Grade D");
    }
    else if(marks>=55 && marks<69) //for Grade C
    {
        printf("Grade C");
    }
    else if(marks>=70 && marks<84)// for Grade B
    {
        printf("Grade B");
   } 
    // for grade A of (85-100) 
    else
    {
        printf("Grade A");
   } 
}
		
